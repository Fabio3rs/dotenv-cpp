@PACKAGE_INIT@

include(CMakeFindDependencyMacro)

# Verificar se os targets jรก foram importados
if(NOT TARGET dotenv::dotenv_lib)
    include("${CMAKE_CURRENT_LIST_DIR}/dotenvTargets.cmake")
endif()

# Verificar componentes solicitados
set(_dotenv_supported_components "")

foreach(_comp ${dotenv_FIND_COMPONENTS})
    if (NOT _comp IN_LIST _dotenv_supported_components)
        set(dotenv_FOUND False)
        set(dotenv_NOT_FOUND_MESSAGE "Unsupported component: ${_comp}")
    endif()
endforeach()

check_required_components(dotenv)
